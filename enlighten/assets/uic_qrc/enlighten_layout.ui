QDateEdit {
  selection-background-color: hsl(0, 0%, 70%);
  border-style: solid;
  border: 1px solid #32414B;
  border-radius: 4px;
  padding-top: 2px;
  padding-bottom: 2px;
  padding-left: 4px;
  padding-right: 4px;
  min-width: 10px;
}

QDateEdit:on {
  selection-background-color: hsl(0, 0%, 70%);
}

QDateEdit::drop-down {
  subcontrol-origin: padding;
  subcontrol-position: top right;
  width: 20px;
  border-top-right-radius: 3px;
  border-bottom-right-radius: 3px;
}

QDateEdit::down-arrow {
  image: url(":/qss_icons/images/darkstyle_icons/arrow_down_disabled.png");
  height: 12px;
  width: 12px;
}

QDateEdit::down-arrow:on, QDateEdit::down-arrow:hover, QDateEdit::down-arrow:focus {
  image: url(":/qss_icons/images/darkstyle_icons/arrow_down.png");
}

QDateEdit QAbstractItemView {
  background-color: #383838;
  border-radius: 4px;
  border: 1px solid #32414B;
  selection-background-color: hsl(0, 0%, 70%);
}

/* what extends this? */

QAbstractView:hover {
  border: 1px solid hsl(0, 0%, 60%);
  color: #F0F0F0;
}

QAbstractView:selected {
  background: hsl(0, 0%, 70%);
  color: #32414B;
}

PlotWidget {
  padding: 0;
}

