/* QPushButton ------------------------------------------------------------
https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qpushbutton
--------------------------------------------------------------------------- */

/* Most GUI buttons are wrapping images, so can be fairly tight */
QPushButton {
    border: 1px solid #333;
    background-color: qlineargradient(spread:pad, 
        x1:0.5, y1:1, 
        x2:0.5, y2:0, 
        stop:0 hsl(0, 0%, 26%),
        stop:1 hsl(0, 0%, 37%));
    border-radius: 5px;
}

QPushButton:hover {
    border: 1px solid hsl(0, 0%, 60%);
}

/* This gets overridden down in the [wpBox] stuff at the bottom */
QPushButton:disabled 
{
    color: #999;
    background-color: hsl(0, 0%, 66%);
    border: 1px solid #333;
}

QPushButton:checked {
  background-color: #333;
  border: 1px solid #333;
  border-radius: 4px;
  padding: 3px;
}

QPushButton:checked:selected {
  background-color: #333;
  color: #32414B;
}

QPushButton:checked:hover {
  border: 1px solid hsl(0, 0%, 60%);
  color: #F0F0F0;
}

QPushButton::menu-indicator {
  subcontrol-origin: padding;
  subcontrol-position: bottom right;
  bottom: 4px;
}

QPushButton:pressed {
  background-color: #383838;
  border: 1px solid #383838;
}

QPushButton:pressed:hover {
  border: 1px solid hsl(0, 0%, 60%);
}

QPushButton:selected {
  background: #333;
  color: #eee;
}

/* QInputDialog -----------------------------------------------------------
--------------------------------------------------------------------------- */

QInputDialog {
    background-color: #383838;
}

QDialog {
    background-color: #383838;
}

/* Dialog Ok/Cancel buttons should be big and obvious */
QDialog QPushButton {
    padding-top: 10px;
    padding-bottom: 10px;
    padding-left: 30px;
    padding-right: 30px;
